CREATE TABLE `proyecto_integrador`.`usuarios` ( `id` INT NOT NULL AUTO_INCREMENT , `nombre_usuario` VARCHAR(255) NOT NULL , `apellido_usuario` VARCHAR(255) NOT NULL , `num_seguidores` INT NULL , `num_comentarios_hechos` INT NULL , `cant_productos` INT NULL , `url_imagen_usuario` VARCHAR(255) NULL , `mail` VARCHAR(255) NOT NULL , `fecha` DATE NOT NULL , `texto_usuario` VARCHAR(255) NULL , PRIMARY KEY (`id`)) ENGINE = InnoDB;
